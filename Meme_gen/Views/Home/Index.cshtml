@model IEnumerable<meme_gen.Models.MemeTemplate>

<div class="masonry-grid">
    @foreach (var meme in Model)
    {
        <div class="meme-card">
            <a href="@Url.Action("Edit", "Meme", new { id = meme.Id })">
                <img src="@meme.ImagePath" alt="@meme.Title" class="meme-image" />
            </a>
        </div>
    }
</div>